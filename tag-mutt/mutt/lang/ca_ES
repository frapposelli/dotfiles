# IMAP folders
set my_drafts="Borradors"
set my_drafts_noquote="Borradors"
set my_sent="Elements<quote-char><space>enviats"
set my_sent_noquote="Elements enviats"
set my_trash="Elements<quote-char><space>eliminats"
set my_trash_noquote="Elements eliminats"

# Quotation and date formats
set attribution="El %d, %n ha escrit:"
set date_format="!%m/%d/%Y %H:%M"

# Hours and dates
set charset="utf-8"
set assumed_charset="utf-8"
set send_charset="utf-8:us-ascii"
